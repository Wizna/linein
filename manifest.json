{
  "manifest_version": 2,

  "name": "Line in",
  "description": "This extension shows comments inline",
  "version": "0.1.1",

  "browser_action": {
    "default_icon": "resources/icon.png",
    "default_popup": "mypopup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}